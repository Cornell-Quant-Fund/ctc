# Production Environment Variables
# Copy this file to .env.prod and fill in your production values

# GitHub Container Registry
GITHUB_REPOSITORY=your-username/ctc

# Database Configuration
POSTGRES_DB=trading_competition
POSTGRES_USER=trading_user
POSTGRES_PASSWORD=your-secure-password-here

# Application Settings
ALLOW_ANY_API_KEY=false

# Frontend URLs (update with your production domain)
NEXT_PUBLIC_API_URL=https://your-domain.com/api/v1
NEXT_PUBLIC_WS_URL=/ws/v1/market-data

# Nginx Ports
HTTP_PORT=80
HTTPS_PORT=443

# Optional: Custom ports for development
# HTTP_PORT=8080
# HTTPS_PORT=8443
